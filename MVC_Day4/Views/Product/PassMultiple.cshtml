<h2>Product List (Using ViewBag)</h2>
<table>
    <thead>
        <tr>
            <th>Code</th>
            <th>Name</th>
            <th>Quantity</th>
            <th>Details</th>
            <th>Price</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var product in ViewBag.product1)
        {
            <tr>
                <td>@product.PCode</td>
                <td>@product.Name</td>
                <td>@product.QtyInStock</td>
                <td>@product.Details</td>
                <td>@product.Price</td>
            </tr>
        }
    </tbody>
</table>
